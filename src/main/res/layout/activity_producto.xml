<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Producto">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/lblProducto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Producto">

        </TextView>
        <TextView
            android:id="@+id/lblVenta"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Total Precio Venta">

        </TextView>
        <TextView
            android:id="@+id/lblCompra"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Total Precio Compra">

        </TextView>
        <TextView
            android:id="@+id/lblGanancia"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Total Ganancia">

        </TextView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">
            <Button
                android:id="@+id/btnCalcular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Calcular">

            </Button>
            <Button
                android:id="@+id/btnRegresar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Regresar">

            </Button>
        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>